def area_of_circle(r):
    pi = 3.14
    return pi * r ** 2
def sum2(start, end, func):
    total = 0
    for x in range(start, end):
        total += func(x)
    return total
def func1(x):
    return x
def func2(x):
    return  x ** 2 + 1
print(area_of_circle(3))
print(sum2(1, 11, func1))
print(sum2(1, 101, func2))